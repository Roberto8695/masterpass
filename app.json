{
  "expo": {
    "name": "MasterPass",
    "slug": "MasterPass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/masterpass.png",
    "scheme": "masterpass",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/masterpass.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rcez.masterpass",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/masterpass.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/masterpass.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Permite que $(PRODUCT_NAME) use Face ID para autenticaci√≥n segura."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cad1e67d-25b3-47b0-ad60-b5368d70b515"
      }
    }
  }
}
