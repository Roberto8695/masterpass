{
  "expo": {
    "name": "MasterPass",
    "slug": "MasterPass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/masterpass.webp",
    "scheme": "masterpass",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/masterpass.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rcez.masterpass",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/masterpass.webp",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/masterpass.webp"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
